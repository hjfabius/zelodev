MEDV03:
                LD      E,(IX+00H)
                LD      D,(IX+01H)
                ADC     HL,DE
                INC     IX
                INC     IX
                DJNZ    MEDV03
                

                LD      (DIVDEN),HL
                LD      BC,0004H        ;WWNN
                LD      (DIVSOR),BC     ;
		CALL    DIVIS
		LD      HL,(QUOZIE)

		LD      (VELMED),HL                 

		LD      DE,(VELPRE)     ;SE NON CAMBIA
		SBC     HL,DE           ;NON AGGIORNO
                JR      Z,MEDV05        ;IL DISPLAY ZZZ
                JR      NC,MEDV04
		CCF

