                ;MEDIA VELOCITA SU QUATTRO LETTURE WWAA WWNN

                LD      B,03H           ;WWAA WWNN
                LD      IX,MVEL00       ;
                LD      DE,0004H        ;WWNN
                ADD     IX,DE
MEDV01:
                LD      E,(IX)          ;DATI LETTI
                LD      D,(IX+01H)      ;INSERENDO
                LD      (IX+02H),E      ;IL NUOVO
                LD      (IX+03H),D      ;DATO AL
                DEC     IX              ;PRIMO POSTO
                DEC     IX              ;ED ELIMINANDO
                DJNZ    MEDV01          ;L'ULTIMO
                INC     IX
                INC     IX


                LD      DE,(ISTVEL)      ;
                LD      (MVEL00),DE

                LD      B,04H            ;SOMMA DELLE  WWNN
                JR      NC,MEDV02        ;4 CELLE  WWAA   WWNN
                CCF                      ; 
