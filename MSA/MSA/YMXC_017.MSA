		CALL    BIN7
                CALL    RITDIS
                CALL    RITDIS


                LD      DE,(TCICAT)
		LD      (BINSET),DE
                LD      A,(LINGUA)
                CP      00H
                JR      Z,TRCM1
                CP      01H
                JR      Z,TRCI1
                CP      02H
                JR      Z,TRCF1
                CP      03H
                JR      Z,TRCGB1
                CP      04H
                JR      Z,TRCE1
                CP      05H
