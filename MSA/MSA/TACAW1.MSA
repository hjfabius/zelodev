NOPRI1:         
                LD      A,(MEMC)        ;QUANDO NO PRINT
                RES     5,A             ;IMPONGO SOLO
                LD      (MEMC),A        ;LETTURE LON-
                OUT     (PC),A          ;GITUDINALI
AZZTOT:					;PUUU
                LD      A,00H           ;ALL'USCITA VVVV
                LD      (MAVA00),A      ;VIENE AZZERATO VVVV
                LD      HL,MAVA00       ;TUTTO CIO'CHE VVVV
                LD      DE,MAVA00+01H   ;RIGUARDA LA  VVVV
