                LD      (SECONB),A
                LD      A,0CH
                LD      (VALORX),A
                JR      MECI04
MECI01:
                LD      A,(CICLO)
                CP      11H             ;= 17
                JR      NC,MECI02
                LD      A,0FH
                LD      (PRIMOB),A
                INC     A
                LD      (SECONB),A
                LD      A,1CH           ;= 28
                LD      (VALORX),A
                JR      MECI04
MECI02:
                LD      A,(CICLO)
                CP      21H             ;= 33
                JR      NC,MECI03
                LD      A,1FH           ;= 31
                LD      (PRIMOB),A
                INC     A
                LD      (SECONB),A
                LD      A,3CH           ;= 60
                LD      (VALORX),A
                JR      MECI04
MECI03:
                LD      A,2FH           ;= 47
                LD      (PRIMOB),A
                INC     A
                LD      (SECONB),A
                LD      A,5CH           ;= 92
                LD      (VALORX),A
MECI04:
                RET

		;CENTRATURA DOPO FASE CON OSCILLOSCOPIO ESTERNO FF57

CENTRO:      
		LD	A,(R_CENTR)	;BLOCCA CENTRO PER
		CP	00H		;4 GIRI ALL'ACCENSIONE
		JR	NZ,EX_CENTRO	;EVITANDO CASINI IN FASES1
		
		EXX                     ;SOTTRAGGO IL VA- FF57
                LD      HL,(FASES1)     ;LORE DI 1/2 GATE
		LD      BC,(MEZGA)      ;DAL VALORE LETTO
                SBC     HL,BC           ;SE IL RISULTATO
		JR      C,CENTR1        ;E'POSITIVO LO
		JR      CENTR2          ;MEMORIZZO IN
CENTR1:         CCF                     ;IN FASE.
		LD      HL,1000H        ;SE E'NEGATIVO
		SBC     HL,BC           ;SOTTRAGGO DA 4096
                LD      BC,(FASES1)     ;IL 1/2 GATE E IL
		ADD     HL,BC           ;RESTO LO SOMMO
CENTR2:         LD      (FASE),HL       ;AL VALORE LETTO.
		LD      A,H
		OUT     (20H),A
		LD      A,L
		OUT     (10H),A
;		LD      A,04H           ;4 GIRI ATTESA
;		LD      (MEMRIF),A      ;PER MEMORIZZARE
		EXX                     ;IL RIFERIMENTO
		JR	EX_CENTRO1
EX_CENTRO:	
		LD	A,(R_CENTR)
		DEC	A
		LD	(R_CENTR),A
EX_CENTRO1:
		RET
		

