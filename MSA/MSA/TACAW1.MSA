                LD      IX,MVEL00       ;
                LD      DE,0004H        ;WWNN
                ADD     IX,DE
MEDV01:
                LD      E,(IX)          ;DATI LETTI
                LD      D,(IX+01H)      ;INSERENDO
                LD      (IX+02H),E      ;IL NUOVO
                LD      (IX+03H),D      ;DATO AL
                DEC     IX              ;PRIMO POSTO
                DEC     IX              ;ED ELIMINANDO
                DJNZ    MEDV01          ;L'ULTIMO
                INC     IX
                INC     IX


                LD      DE,(ISTVEL)      ;
                LD      (MVEL00),DE

                LD      B,04H            ;SOMMA DELLE  WWNN
                JR      NC,MEDV02        ;4 CELLE  WWAA   WWNN
                CCF                      ; 
MEDV02:         LD      HL,0000H
MEDV03:
                LD      E,(IX+00H)
                LD      D,(IX+01H)
                ADC     HL,DE
                INC     IX
                INC     IX
                DJNZ    MEDV03
                

                LD      (DIVDEN),HL
                LD      BC,0004H        ;WWNN
                LD      (DIVSOR),BC     ;
		CALL    DIVIS
		LD      HL,(QUOZIE)

		LD      (VELMED),HL                 

		LD      DE,(VELPRE)     ;SE NON CAMBIA
		SBC     HL,DE           ;NON AGGIORNO
                JR      Z,MEDV05        ;IL DISPLAY ZZZ
                JR      NC,MEDV04
		CCF

MEDV04:         LD      HL,(VELMED)

		LD      (BINSET),HL     ;ZZZ
