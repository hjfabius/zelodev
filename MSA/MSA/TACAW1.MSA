CORREZ:      
		
		
		LD      HL,(ERMEDI)     ;SE ZONA MORTA E'
		LD      DE,(ZONA)       ;> DELL'ERRORE
		SBC     HL,DE           ;NON CORREGGO
		JR      NC,CORREZIONI_1
		CCF
		LD	A,(MEMC)		;SE SONO IN 
		BIT	2,A			;MANUALE SALTO
		JR      NZ,CORREZIONI_2
		RES	7,A
		JR	CORREZIONI_3
CORREZIONI_2:
		RES     0,A             
		RES     1,A   
		LD      (MEMC),A        
		OUT     (PC),A          

		LD	HL,(FASES1)		;CONTROLLO SE I DUE
		LD	DE,(FASES2)		;SONO A 180°FRA DI LORO
		SBC	HL,DE			;SE LO SONO EVITO DI
		JR	NC,CORREZIONI_5		;FARE USCIRE OK TAGLIA
		CCF				;QUANDO FASES2 PASSANDO
		EX	DE,HL			;DA +/- 180° MI DA PER
		LD	DE,(FASES1)		;UN ATTIMO ERRORE ZERO
		SBC	HL,DE
CORREZIONI_5:
		LD	DE,0400H
		SBC	HL,DE
		JR	NC,CORREZIONI_1
		CCF
		LD	A,(MEMC)
		SET	7,A
CORREZIONI_3:
		LD      (MEMC),A        
		OUT     (PC),A          
		JP      CORREZIONI_END
;
;
;		;#####################################################################
;		;# CALCOLO CORREZIONE MOTORE                                         #
;		;# [Ea x C/10 + (Ea - Ep) x D] x G                                   #
;		;# [Ea x (C/10 + D) x G] - [Ep x D x G]	                             #
;		;# CORRATT = 80000000h + [Ea x (C/10 + D) x G] - [Ep x D x G]        #
;		;#####################################################################
;
;
CORREZIONI_1:   
		LD      A,(MEMC)        
		RES     7,A             
		LD      (MEMC),A        
		OUT     (PC),A          
		


		LD      A,(MATTES)		;CONTROLLO CICLO
		DEC     A			;DI ATTESA
		LD      (MATTES),A
		CP      00H
		JP      NZ,CORREZIONI_END


                LD      A,(CICLO)		;RICARICO IL CI-
		LD      (MATTES),A		;CLO DI ATTESA.

		LD	DE,(ERMEDI)		;WWWW
		LD	A,D
		CP	00H
		JR	NZ,CORREZIONI_4
		LD	A,E
		CP	00H
		JR	Z,KRAPIN		;WWWW

CORREZIONI_4:         
		LD	A,(MEMC)		;SE SONO IN 
		BIT	2,A			;MANUALE SALTO
		JP      Z,CORREZIONI_END


                LD      A,37H			;CONTROLLO CTC1   
		OUT     (71H),A         
                LD      A,10H			;COSTANTE DI TEMPO
		OUT     (71H),A			
		LD      A,0D7H			;CONTROLLO CTC2
		OUT     (72H),A			;IL VALORE GUADAGNO 
                LD      A,(GUADA)		;CORRISPONDE A mS WWWW  
		OUT     (72H),A


		LD      A,(MSCORR)		;VEDO IN CHE
		CP      01H			;SENSO CORREG.
		JR      NZ,AUTRIT

AUTAVA:         LD      A,(MEMC)        ;CORREZIONE IN
		RES     1,A		;AVANTI
		SET     0,A
		RES	7,A
		LD      (MEMC),A
		OUT     (PC),A

		JR      CORREZIONI_END

AUTRIT:

AUTRI1:         LD      A,(MEMC)        ;CORREZIONE IN
		RES     0,A		;RITARDO
		SET     1,A
		RES	7,A
		LD      (MEMC),A
		OUT     (PC),A

		
CORREZIONI_END:         
		RET
