		LD      A,(ALLPRI)      ;CONTO GLI AVVISI CCAA
		CP      00H             ;PRIMA DI ATTIVA- 
		JR      Z,CAVAL2        ;RE L'ALLARME
		DEC     A
		LD      (ALLPRI),A
		JR      CAVAL1
CAVAL2:         LD      A,(MEMC)
		SET     3,A             ;ATTIVO ALLARME
		LD      (MEMC),A
